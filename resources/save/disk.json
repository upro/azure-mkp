{    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",    "contentVersion": "1.0.0.0",    "parameters": {        "disks_zeus_beeshake_com_name": {            "defaultValue": "zeus.beeshake.com",            "type": "String"        },        "snapshots_beeshake_5_18_6_externalid": {            "defaultValue": "/subscriptions/4e02df79-87cb-4e6c-8810-d6e2c2a7ea38/resourceGroups/BEESHAKERESOURCEGROUP/providers/Microsoft.Compute/snapshots/beeshake-5.18.6",            "type": "String"        }    },    "variables": {},    "resources": [        {            "type": "Microsoft.Compute/disks",            "apiVersion": "2019-07-01",            "name": "[parameters('disks_zeus_beeshake_com_name')]",            "location": "francecentral",            "tags": {                "product": "beeshake",                "production": "no",                "customer": "demo"            },            "sku": {                "name": "Premium_LRS",                "tier": "Premium"            },            "properties": {                "osType": "Linux",                "hyperVGeneration": "V1",                "creationData": {                    "createOption": "Copy",                    "sourceResourceId": "[parameters('snapshots_beeshake_5_18_6_externalid')]"                },                "diskSizeGB": 32,                "diskIOPSReadWrite": 120,                "diskMBpsReadWrite": 25,                "encryption": {                    "type": "EncryptionAtRestWithPlatformKey"                }            }        }    ]}