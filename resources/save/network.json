{    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",    "contentVersion": "1.0.0.0",    "parameters": {        "networkInterfaces_zeus_beeshake_com577_name": {            "defaultValue": "zeus.beeshake.com577",            "type": "String"        },        "publicIPAddresses_zeus_beeshake_com_ip_externalid": {            "defaultValue": "/subscriptions/4e02df79-87cb-4e6c-8810-d6e2c2a7ea38/resourceGroups/BeeshakeResourceGroup/providers/Microsoft.Network/publicIPAddresses/zeus.beeshake.com-ip",            "type": "String"        },        "virtualNetworks_BeeshakeResourceGroup_vnet_externalid": {            "defaultValue": "/subscriptions/4e02df79-87cb-4e6c-8810-d6e2c2a7ea38/resourceGroups/BeeshakeResourceGroup/providers/Microsoft.Network/virtualNetworks/BeeshakeResourceGroup-vnet",            "type": "String"        },        "networkSecurityGroups_Beeshake_SG_externalid": {            "defaultValue": "/subscriptions/4e02df79-87cb-4e6c-8810-d6e2c2a7ea38/resourceGroups/BeeshakeResourceGroup/providers/Microsoft.Network/networkSecurityGroups/Beeshake-SG",            "type": "String"        }    },    "variables": {},    "resources": [        {            "type": "Microsoft.Network/networkInterfaces",            "apiVersion": "2019-11-01",            "name": "[parameters('networkInterfaces_zeus_beeshake_com577_name')]",            "location": "francecentral",            "tags": {                "product": "beeshake",                "production": "no",                "customer": "zeus"            },            "properties": {                "ipConfigurations": [                    {                        "name": "ipconfig1",                        "properties": {                            "privateIPAddress": "172.17.1.5",                            "privateIPAllocationMethod": "Dynamic",                            "publicIPAddress": {                                "id": "[parameters('publicIPAddresses_zeus_beeshake_com_ip_externalid')]"                            },                            "subnet": {                                "id": "[concat(parameters('virtualNetworks_BeeshakeResourceGroup_vnet_externalid'), '/subnets/default')]"                            },                            "primary": true,                            "privateIPAddressVersion": "IPv4"                        }                    }                ],                "dnsSettings": {                    "dnsServers": []                },                "enableAcceleratedNetworking": false,                "enableIPForwarding": false,                "networkSecurityGroup": {                    "id": "[parameters('networkSecurityGroups_Beeshake_SG_externalid')]"                }            }        }    ]}